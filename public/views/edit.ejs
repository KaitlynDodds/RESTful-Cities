<h2>Edit City: <%= city.name %></h2>

<form class="" action="/cities/<%= city._id %>?_method=PUT" method="POST">
  <input type="text" name="city[name]" value="<%= city.name %>">
  <input type="text" name="city[state]" value="<%= city.state %>">
  <input type="number" name="city[population]" value="<%= city.population %>">
  <input type="text" name="city[image]" value="<%= city.image %>">
  <input type="submit">
</form>
